+               BUG_ON(desc_data->skb_top == NULL);
                if (pkt_len > pdata->rx_buffer_len) {
                   skb_fill_page_desc(desc_data->skb_top,
                      skb_shinfo(desc_data->skb_top)->nr_frags,
